{
	"compilerOptions": {
		"incremental": true,
		"tsBuildInfoFile": "./.lint-cache/.tsbuildinfo",
		"target": "es2016",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "./dist",
		"allowJs": true,
		"checkJs": true,
		"pretty": true
	},
	"include": ["./src/**/*.ts"],
	"exclude": ["./dist"]
}
